          <div class="wrapper">
            <h3> Log file Display Box</h3>
          <center>
            <table border="1">
              <thead>
        
                <tr>
                  <th>Operator Name</th>
                  <td><input type="text" name="operatorName" id="operatorName" placeholder="Type your name here"></td>
                            
                </tr>
        
              </thead>
        
              <tbody>
                <tr>
                  <th>Log message</th>
                  <td><input type="text" name="logNote" id="logNote" placeholder="Leave your message here"></td>
                            
                </tr>
        
                
        
                <tr id="buttonClick">
                            <div id="timeStamp"></div>
                  <td colspan="2"><input type="button" class="btn btn-primary" name="button" id="btn" value="Create Log message" onclick="AddRow()"></td>
                </tr>
              </tbody>
            </table>
        
            <table border="1" border-style="round" id="tableDisp">
              <thead>
                <tr>
                  <th>Operator Name </th>
                  <th>Log message</th>
                  <th>Time stamp</th>
                </tr>
              </thead>
            </table>
          </center>
          
          <script>
              
            let listOperator = [];
            let listLog = [];
                let  listTime = [];
            
        
            var i = 1;
            var j = 0;
        
            function AddRow(){
        
              var AddRown = document.getElementById('tableDisp');
              var NewRow = AddRown.insertRow(i);
                    // var timeStamp2= document.getElementById("timeStamp");
        
                    //nice job!
                    document.getElementById("timeStamp").value= new Date(document.lastModified);
        
              listOperator[j] = document.getElementById("operatorName").value;
              listLog[j] = document.getElementById("logNote").value;
                    
                   
                    
                    // <div id="timeStamp"></div>
                    // <div id="timeStamp"></div>
              
                    listTime[j] = document.getElementById("timeStamp").value;
              
              var tile01 = NewRow.insertCell(0);
              var tile02 = NewRow.insertCell(1);
                    var tile03 = NewRow.insertCell(2);
            
        
              tile01.innerHTML = listOperator[j];
              tile02.innerHTML = listLog[j];
                    tile03.innerHTML = listTime[j];
            
        
              i++;
              j++;
            }
           

          </script>
            </div>

            <!-- grid -->

            <p>
              <button id="fileUploading" >Load Manifest.txt file</button>
            </p>
            <div class="fileUpload">
         
              <script>
              function fileUploading() {
                  loadStrings('rainbow.txt', fileLoaded);
              }
              
              function fileLoaded(data) {
                  // txt = data;
                  createP(join(data, '<br/>'));
              }
              
              function fileRead(file) {
                  createP(file.name + ' ' + file.size + ' ' + file.type);
              
                  if (file.type == 'text') {
                  createP(file.data);
                  } else {
                  createP('Warning: Unsupported file type.');
                  }
                
              }
              
              function setup() {
                  noCanvas();
              
                  createFileInput(fileRead);
              
                  var button = select('#loadfile');
                  button.mousePressed(fileUploading);
              
                 
              }
              </script>
